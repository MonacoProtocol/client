{
  "name": "@monaco-protocol/client",
  "version": "0.0.1",
  "description": "Interface package for the Monaco Protocol on Solana",
  "author": "Monaco Protocol",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/MonacoProtocol/client.git"
  },
  "bugs": {
    "url": "https://github.com/MonacoProtocol/client/issues"
  },
  "homepage": "https://github.com/MonacoProtocol/client#readme",
  "main": "src/index.js",
  "module": "src/index.js",
